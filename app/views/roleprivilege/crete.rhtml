
<p><br>
<% form_tag do %>
<label for="role">Available Roles</label><br>
<%=@roles=Role.find(:all , :order => "role").map {|r|[r.role]}
select("role" ,"role", @roles)
 %>
<br><br>
<label for="privilege">Available Privileges</label><br>
<%=@privileges=Privilege.find(:all , :order => "privilege").map {|p|[p.privilege]}
select("privilege" ,"privilege", @privileges)
 %>
<%=submit_tag "create" %> 
<% end %>
